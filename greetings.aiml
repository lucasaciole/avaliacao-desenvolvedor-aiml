<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
  <!-- insert your AIML categories here -->
  <category>
      <pattern>OI ^</pattern>
      <template>Olar, eu sou o Caipirot, bão te conhecê! Qual é o seu nome?</template>
  </category>
  
  <category>
      <pattern>*</pattern>
      <that>QUAL _ O SEU NOME</that>
      <template>Muito prazer viu, <set name="name"><star/></set>. Que cê precisa hoje?</template>
  </category>
  
  <category>
      <pattern>_ MEU NOME</pattern>
      <template>
          <condition name="name">
            <li value="unknown">Tem como eu sabe não, cê não falo!</li>
            <li>Cê eu num tivé minganado, u cê falo que se chama <get name="name" />.</li>
          </condition>
      </template>
  </category>

  <category>
    <pattern>CONTE DE <set>number</set> ATÉ <set>number</set></pattern>
    <template>
      <think>
          <set name="countvar"><star/></set>
          <set name="count"><map><name>predecessor</name><star index="0"/></map></set>
      </think>
      <condition name="count">
        <li><value><star index="2"/></value></li>
        <li><set name="count"><map><name>successor</name><get name="count"/></map></set><loop/>,</li>
      </condition>
    </template>
  </category>
</aiml>